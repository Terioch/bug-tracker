{
  "ConnectionStrings": {
    "DefaultConnection": "Host={postgresHost};Port={postgresPort};Username={postgresUsername};Password={postgresPassword};Pooling=true;sslmode=Prefer;Trust Server Certificate=true"
  },  
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },  
  "AllowedHosts": "*"
}